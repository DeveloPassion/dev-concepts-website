{
  "private": true,
  "name": "dev-concepts-website",
  "description": "Dev Concepts Website",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeveloPassion/dev-concepts-website.git"
  },
  "bugs": {
    "url": "https://github.com/DeveloPassion/dev-concepts-website/issues"
  },
  "homepage": "https://github.com/DeveloPassion/dev-concepts-website#readme",
  "author": "dSebastien <seb@dsebastien.net> (https://www.dsebastien.net)",
  "engines": {
    "node": ">=12.10.0"
  },
  "lint-staged": {
    "*.{css,js,json,md,pcss,scss,ts}": [
      "prettier --write --html-whitespace-sensitivity strict",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "npm run develop",
    "build:prod": "gatsby build --prefix-paths && node ./csp-util.js",
    "build:storybook": "build-storybook",
    "check-deps": "npx npm-check",
    "clean": "gatsby clean",
    "clean:slate": "npm run rimraf -- node_modules .tmp .cache public dist",
    "preclean:install": "npm run clean:slate",
    "clean:install": "npm install",
    "deploy": "npm run build:prod && npm run gh-pages",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "format": "prettier --write **/*.{css,js,jsx,json,md,pcss,scss,ts,tsx}",
    "gatsby": "gatsby",
    "gh-pages": "gh-pages --dist public --branch gh-pages",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:css": "stylelint \"./src/**/*.?(pc|sc|c|sa)ss\" --formatter \"string\"",
    "lint:fix": "npm run lint:ts --fix",
    "lint:ts": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "lint:watch": "esw -w . --ext ts --ext tsx --ext js --ext jsx",
    "lint-staged": "lint-staged",
    "postinstall": "node ./node_modules/husky/lib/bin.js install && rm -rf node_modules/@types/react-native",
    "prettier": "npm run format",
    "rimraf": "npx rimraf",
    "serve": "npm run serve:dev",
    "serve:dev": "npm run build:dev",
    "serve:prod": "npm run build:prod && gatsby serve",
    "start": "npm run serve:dev",
    "storybook": "start-storybook -p 6006",
    "tailwind": "tailwind",
    "test": "jest",
    "test:watch": "jest --watch",
    "tsc": "tsc",
    "update": "ncu -u"
  },
  "dependencies": {
    "@martin_hotell/rex-tils": "2.1.1",
    "framer-motion": "2.9.4",
    "gatsby": "2.24.91",
    "gatsby-image": "2.4.21",
    "gatsby-plugin-csp": "1.1.3",
    "gatsby-plugin-google-analytics": "2.3.19",
    "gatsby-plugin-manifest": "2.4.37",
    "gatsby-plugin-netlify": "2.3.25",
    "gatsby-plugin-offline": "3.2.38",
    "gatsby-plugin-postcss": "3.0.4",
    "gatsby-plugin-purgecss": "5.0.0",
    "gatsby-plugin-react-axe": "0.5.0",
    "gatsby-plugin-react-helmet": "3.3.14",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-sharp": "2.6.43",
    "gatsby-plugin-sitemap": "2.4.17",
    "gatsby-plugin-styled-components": "3.3.14",
    "gatsby-plugin-stylelint": "3.2.0",
    "gatsby-plugin-typescript": "2.4.25",
    "gatsby-source-filesystem": "2.3.37",
    "gatsby-transformer-sharp": "2.5.20",
    "gh-pages": "3.1.0",
    "io-ts": "2.2.12",
    "modern-css-reset": "1.1.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "styled-components": "5.2.1",
    "svg-react-loader": "0.4.6",
    "tailwindcss": "1.9.6",
    "twin.macro": "1.12.0",
    "use-media": "1.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-knobs": "6.0.28",
    "@storybook/addon-links": "6.0.28",
    "@storybook/addons": "6.0.28",
    "@storybook/react": "6.0.28",
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "11.1.0",
    "@types/jest": "26.0.15",
    "@types/react": "16.9.55",
    "@types/react-dom": "16.9.9",
    "@types/react-helmet": "6.1.0",
    "@types/styled-components": "5.1.4",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "autoprefixer": "10.0.1",
    "babel-jest": "26.6.1",
    "babel-loader": "8.1.0",
    "babel-plugin-macros": "2.8.0",
    "babel-plugin-styled-components": "1.11.1",
    "babel-preset-gatsby": "0.5.14",
    "babel-preset-react-app": "10.0.0",
    "eslint": "7.12.1",
    "eslint-config-gojutin": "0.1.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-immutable": "1.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-watch": "7.0.0",
    "htmlparser2": "5.0.1",
    "husky": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.1",
    "jest-styled-components": "7.0.3",
    "lint-staged": "10.5.0",
    "node-sass": "4.14.1",
    "npm-check-updates": "9.2.3",
    "postcss": "8.1.4",
    "postcss-import": "13.0.0",
    "prettier": "2.1.2",
    "replace-in-file": "6.1.0",
    "stylelint": "13.7.2",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-webpack-plugin": "2.1.1",
    "ts-jest": "26.4.3",
    "tslint-immutable": "6.0.1",
    "typescript": "4.0.5"
  }
}
